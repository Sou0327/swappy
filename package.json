{
  "name": "swappy",
  "private": false,
  "version": "0.1.0-beta",
  "license": "MIT",
  "description": "Self-hosted multi-chain wallet platform for privacy-conscious users",
  "type": "module",
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "check-all": "npm run type-check && npm run lint",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:ui": "NODE_OPTIONS='--max-old-space-size=8192' vitest --ui",
    "test:run": "NODE_OPTIONS='--max-old-space-size=8192' vitest run",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=8192' vitest --watch",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:deno": "SUPABASE_URL=https://test.supabase.co SUPABASE_SERVICE_ROLE_KEY=test-key deno test --allow-all supabase/functions/**/*.test.ts",
    "test:deno:watch": "SUPABASE_URL=https://test.supabase.co SUPABASE_SERVICE_ROLE_KEY=test-key deno test --allow-all --watch supabase/functions/**/*.test.ts",
    "tatum:sync": "tsx scripts/tatum-subscription-manager.ts sync",
    "tatum:list": "tsx scripts/tatum-subscription-manager.ts list",
    "tatum:create": "tsx scripts/tatum-subscription-manager.ts create",
    "tatum:delete": "tsx scripts/tatum-subscription-manager.ts delete",
    "tatum:status": "tsx scripts/tatum-subscription-manager.ts status",
    "review": "coderabbit review --config .coderabbit.yaml",
    "review:staged": "coderabbit review --type uncommitted --config .coderabbit.yaml",
    "review:committed": "coderabbit review --type committed --config .coderabbit.yaml",
    "review:plain": "coderabbit review --plain --config .coderabbit.yaml"
  },
